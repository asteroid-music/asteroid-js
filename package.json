{
  "name": "asteroid-js",
  "version": "1.0.0",
  "description": "Asteroid frontend written predominantly in TypeScript with React",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "webpack --config wp.config.js",
    "dev": "webpack-dev-server --config wp.config.js --mode development"
  },
  "author": "OneSlightWeirdo",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-react": "^7.10.4",
    "@types/axios": "^0.14.0",
    "@types/react": "^16.9.46",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "react-test-renderer": "^16.13.1",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "axios": "^0.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "tsx",
      "ts"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
